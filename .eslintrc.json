{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "standard-with-typescript",
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": ["@typescript-eslint"],
  "ignorePatterns": [
    "dist",
    "node_modules",
    "*.d.ts",
    "apidocs.json",
    "tsconfig.json",
    "package.json",
    "package-lock.json"
  ],
  "rules": {}
}
